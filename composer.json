{
  "name": "beapi/facetwp-yearly",
  "description": "Add custom facet to filter your posts by yearly archive",
  "license": "GPL-3.0-or-later",
  "type": "wordpress-plugin",
  "keywords": [
    "facetwp",
    "yearly",
    "archive"
  ],
  "authors": [
    {
      "name": "Be API",
      "email": "technical@beapi.fr"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "composer/installers": "^1.0 || ^2.0"
  },
  "autoload": {
    "psr-4": {
      "Beapi\\Facetwp\\Date\\": "classes"
    }
  },
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
    "humanmade/psalm-plugin-wordpress": "^3.0",
    "overtrue/phplint": "^4.0",
    "php-parallel-lint/php-parallel-lint": "^1.3",
    "phpcompatibility/phpcompatibility-wp": "^2.1",
    "phpro/grumphp-shim": "^1.5",
    "roave/security-advisories": "dev-latest",
    "vimeo/psalm": "^5.20",
    "wp-coding-standards/wpcs": "^3.0"
  },
  "scripts": {
    "cs": "./vendor/bin/phpcs",
    "cbf": "./vendor/bin/phpcbf",
    "psalm": "./vendor/bin/psalm"
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "phpro/grumphp-shim": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
